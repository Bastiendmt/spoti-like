{
  "name": "spotify-clone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "yarn biome check src/",
    "lint:fix": "yarn biome check src/ --apply",
    "format": "yarn biome format src/ --write",
    "ci": "yarn biome ci src/",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "@biomejs/biome": "^1.7.0",
    "@reduxjs/toolkit": "^2.2.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.4.3",
    "axios": "^1.6.8",
    "fast-average-color": "^9.4.0",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.15.0",
    "sass": "^1.77.6",
    "typescript": "^5.4.2",
    "universal-cookie": "^7.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/qs": "^6.9.10",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.0",
    "jsdom": "^24.0.0",
    "vite": "^5.2.9",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "repository": "https://github.com/bastiendmt/spotify-like-web.git",
  "author": "Bastien DUMONT <bastienxs@gmail.com>",
  "license": "MIT"
}
